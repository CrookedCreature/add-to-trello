{
    "manifest_version": 2,
    "name": "Add to Trello",
    "description": "Add cards to your Trello boards from your browser",
    "version": "0.1",
    "browser_action": {
        "default_icon": "assets/icon.png",
        "default_popup": "popup.html",
        "default_title": "Add Cards to Trello"
    },
    "permissions": [
        "tabs",
        "https://api.trello.com/"
    ],
    "web_accessible_resources": [
        "settings.html"
    ]
}
