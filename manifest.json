{
    "manifest_version": 2,
    "name": "Trello Add Cards",
    "description": "Add cards to your Trello boards easily",
    "version": "1.0",
    "browser_action": {
        "default_icon": "assets/icon.png",
        "default_popup": "index.html",
        "default_title": "Add Cards to Trello"
    },
    "permissions": [
        "tabs",
        "https://ajax.googleapis.com/"
    ]
}
